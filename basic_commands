from opentrons import protocol_api

metadata = {
    'apiLevel': '2.15',
    'protocolName': 'Serial Dilution Tutorial',
    'description': '''This protocol is the outcome of following the
                   Python Protocol API Tutorial located at
                   https://docs.opentrons.com/v2/tutorial.html. It takes a
                   solution and progressively dilutes it by transferring it
                   stepwise across a plate.''',
    'author': 'New API User'
    }

requirements = {"robotType": "OT-1", "apiLevel": "2.15"}

def run(protocol: protocol_api.ProtocolContext):
    tips = protocol.load_labware('tiprack-200ul', "A1")
    reservoir = protocol.load_labware('24-well-plate', 'C1')
    plate = protocol.load_labware('96_flat_imaging','B1')


left_pipette = protocol.load_instrument('p300_single_gen2', 'left', tip_racks=[tips])

row = plate.rows()[0]
left_pipette.transfer(100, reservoir['A2'], row[0], mix_after=(3, 50))
left_pipette.transfer(100, row[:11], row[1:], mix_after=(3, 50))



print("hi")











